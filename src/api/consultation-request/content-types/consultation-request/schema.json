{
  "kind": "collectionType",
  "collectionName": "consultation_requests",
  "info": {
    "singularName": "consultation-request",
    "pluralName": "consultation-requests",
    "displayName": "ConsultationRequest",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "query": {
      "type": "richtext",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending Information",
        "Submitted",
        "Token Issued",
        "Selected",
        "In Progress",
        "Completed",
        "Skipped",
        "Cancelled"
      ],
      "default": "Pending Information",
      "required": true
    },
    "tokenNumber": {
      "type": "string",
      "unique": true,
      "configurable": false
    },
    "issueDate": {
      "type": "datetime"
    },
    "selectedDate": {
      "type": "date"
    },
    "completionDate": {
      "type": "datetime"
    },
    "isQrEmailSent": {
      "type": "boolean",
      "default": false
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "consultation_requests", 
      "required": true
    }
  }
}
